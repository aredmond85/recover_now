<h1>Report Index</h1>
<table>
   <thead>
      <tr>
         <th>Subject</th>
         <th>Description</th>
      </tr>
   </thead>
   <tbody>
      <% @reports.each do |report|%>
      <tr>
         <td><%= report.subject %>
         <td><%= report.description %>
         <td><%= link_to "Edit Report", edit_report_path(report) %>
         <td><%= link_to "Delete Report", report_path(report), method: :delete, data: {confirm: "Are you sure?"} %>
      </tr>
      <% end %>
   </tbody>
</table>
<%= link_to 'New Report', new_report_path %>